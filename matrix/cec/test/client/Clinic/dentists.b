SUBROUTINE Dentists()

Dim @SQL.DATE
Dim @AUTHORIZATION
Dim @DATA.PENDING
Dim @ACCOUNT
Dim @FILENAME
Dim @HDBC

CALL !MESSAGE(@SQL.DATE)
CALL !ADDS(@AUTHORIZATION, @DATA.PENDING)
CALL !ASYNC(@ACCOUNT)
CALL !ANDS(@FILENAME, @HDBC)

IF @SQL.DATE ! @AUTHORIZATION THEN
   ECHO @SQL.DATE ! @AUTHORIZATION MATCHES @ACCOUNT |
        MATCH @FILENAME = "" |
        @MONTH = @HDBC 
END

END     
        
